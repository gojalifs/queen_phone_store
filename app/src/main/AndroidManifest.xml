<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fajar.latihan.queenphonestore">

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--        Activity of Each Product-->
        <activity android:name=".product.samsung.Samsung_Activity" />
        <activity android:name=".product.huawei.Huawei_Activity" />
        <activity android:name=".product.realme.Realme_Activity" />
        <activity android:name=".product.xiaomi.Xiaomi_Activity" />
        <activity android:name=".product.vivo.Vivo_Activity" />
        <activity android:name=".product.iphone.Iphone_Activity" />
        <activity android:name=".product.google.Google_Activity" />
        <activity android:name=".product.honor.Honor_Activity" />
        <activity android:name=".product.oppo.Oppo_Activity" />
    </application>

</manifest>